{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gl-project-case1"
		},
		"GLstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'GLstorage'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://gl-project-key.vault.azure.net/"
		},
		"GLstorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://glstore1123.dfs.core.windows.net/"
		},
		"LandingTrigger_properties_pipeline-live_parameters_fileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"LandingTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ec3999fe-bc0e-4dc1-b992-be2c0a5d558b/resourceGroups/gl-project-live-case-study/providers/Microsoft.Storage/storageAccounts/glstore1123"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "gl-project-case1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"GLstorage_accountKey": {
						"value": "[parameters('GLstorage_accountKey')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"GLstorage_properties_typeProperties_url": {
						"value": "[parameters('GLstorage_properties_typeProperties_url')]"
					},
					"LandingTrigger_properties_pipeline-live_parameters_fileName": {
						"value": "[parameters('LandingTrigger_properties_pipeline-live_parameters_fileName')]"
					},
					"LandingTrigger_properties_typeProperties_scope": {
						"value": "[parameters('LandingTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}